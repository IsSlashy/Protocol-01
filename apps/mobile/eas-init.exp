#!/usr/bin/expect -f

set timeout 120

spawn eas init

expect {
    "*Y/n*" {
        send "y\r"
        exp_continue
    }
    "*y/N*" {
        send "y\r"
        exp_continue
    }
    "*linked*" {
        puts "\n=== EAS project linked successfully ==="
    }
    timeout {
        puts "\nTimeout"
        exit 1
    }
    eof {
        puts "\nEOF reached"
    }
}

puts "Done"
