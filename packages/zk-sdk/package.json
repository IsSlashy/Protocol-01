{
  "name": "@p01/zk-sdk",
  "version": "0.1.0",
  "description": "Zero-knowledge SDK for Protocol 01 shielded transactions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./circuits": {
      "import": "./dist/circuits/index.mjs",
      "require": "./dist/circuits/index.js",
      "types": "./dist/circuits/index.d.ts"
    },
    "./prover": {
      "import": "./dist/prover/index.mjs",
      "require": "./dist/prover/index.js",
      "types": "./dist/prover/index.d.ts"
    },
    "./notes": {
      "import": "./dist/notes/index.mjs",
      "require": "./dist/notes/index.js",
      "types": "./dist/notes/index.d.ts"
    },
    "./merkle": {
      "import": "./dist/merkle/index.mjs",
      "require": "./dist/merkle/index.js",
      "types": "./dist/merkle/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "wasm"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "protocol-01",
    "privacy",
    "zero-knowledge",
    "zk-snark",
    "groth16",
    "shielded",
    "crypto"
  ],
  "author": "Protocol 01 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protocol-01/zk-sdk"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@noble/hashes": "^1.3.3",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "circomlibjs": "^0.1.7",
    "ffjavascript": "^0.3.0",
    "poseidon-lite": "^0.2.0",
    "snarkjs": "^0.7.4"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.11.0",
    "@types/snarkjs": "^0.7.8",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.91.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
